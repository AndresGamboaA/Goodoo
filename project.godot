; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

_global_script_classes=[{
"base": "CustomComponent",
"class": &"App",
"language": &"GDScript",
"path": "res://app.gd"
}, {
"base": "Node",
"class": &"BaseComponent",
"language": &"GDScript",
"path": "res://goodoo/base_component.gd"
}, {
"base": "BaseComponent",
"class": &"BasicComponent",
"language": &"GDScript",
"path": "res://goodoo/basic_component.gd"
}, {
"base": "BaseComponent",
"class": &"CustomComponent",
"language": &"GDScript",
"path": "res://goodoo/custom_component.gd"
}]
_global_script_class_icons={
"App": "",
"BaseComponent": "",
"BasicComponent": "",
"CustomComponent": ""
}

[application]

config/name="Goodoo"
run/main_scene="res://root.tscn"
config/features=PackedStringArray("4.0", "Vulkan Clustered")
run/low_processor_mode=true

[autoload]

Goodoo="*res://goodoo/goodoo.gd"
Goo="*res://goodoo/goo.gd"
Utils="*res://goodoo/utils.gd"
Presets="*res://presets.gd"

[display]

window/size/viewport_width=500
window/size/viewport_height=350
